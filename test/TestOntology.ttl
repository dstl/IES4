<http://test.data#> rdf:type owl:Ontology ;
                     rdfs:comment "A ontology to test shacl shape creation from owl ontology" ;
                     rdfs:label "Test Ontology" .

#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://test.data#testObjectProperty
:testObjectProperty rdf:type owl:ObjectProperty ;
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( :TestClassA
                                                :TestClassB
                                              )
                                ] ;
                    rdfs:range :TestClassC ;
                    rdfs:label "test object property" .


#################################################################
#    Data properties
#################################################################

###  http://test.data#testDataTypeProperty
:testDataTypeProperty rdf:type owl:DatatypeProperty ;
                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( :TestClassA
                                                  :TestClassB
                                                )
                                  ] ;
                      rdfs:range [ rdf:type rdfs:Datatype ;
                                   owl:unionOf ( xsd:boolean
                                                 xsd:date
                                                 xsd:integer
                                               )
                                 ] ;
                      rdfs:label "test data property" .


###  http://test.data#testDatatypeProperty2
:testDatatypeProperty2 rdf:type owl:DatatypeProperty ;
                       rdfs:label "test data property 2" .


#################################################################
#    Classes
#################################################################

###  http://test.data#TestClassA
:TestClassA rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :testObjectProperty ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :TestClassD
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :testObjectProperty ;
                              owl:maxQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                              owl:onClass :TestClassD
                            ] ;
            rdfs:label "Test Class A" .


###  http://test.data#TestClassB
:TestClassB rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :testObjectProperty ;
                              owl:someValuesFrom :TestClassD
                            ] ;
            rdfs:label "Test Class B" .


###  http://test.data#TestClassC
:TestClassC rdf:type owl:Class ;
            rdfs:subClassOf :TestClassA ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :testObjectProperty ;
                              owl:allValuesFrom :TestClassD
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :testObjectProperty ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :TestClassD
                            ] ;
            rdfs:label "Test Class C" .


###  http://test.data#TestClassD
:TestClassD rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :testDatatypeProperty2 ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ;
            rdfs:label "Test Class D" .


